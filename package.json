{
  "name": "proshop-v2",
  "version": "1.0.0",
  "description": "eCommerce application built with the MERN stack",
  "homepage": "https://github.com/yiqu1/proshop-v2#readme",
  "bugs": {
    "url": "https://github.com/yiqu1/proshop-v2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yiqu1/proshop-v2.git"
  },
  "license": "ISC",
  "author": "Ethan Chou",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10"
  }
}
